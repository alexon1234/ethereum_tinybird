
SCHEMA >
    transaction_hash String,
    to_address String,
    from_address String,
    token_address String,
    value Int64,
    block_timestamp DateTime

ENGINE "MergeTree"
ENGINE_PARTITION_KEY "toYear(block_timestamp)"
ENGINE_SORTING_KEY "block_timestamp, cityHash64(to_address), cityHash64(from_address)"
ENGINE_SAMPLING_KEY "cityHash64(to_address), cityHash64(from_address)"
